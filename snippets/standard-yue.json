{
    "Yuescript - Class": {
        "prefix": "class",
        "body": "class $1\n"
    },
    "Yuescript - Extended Class": {
        "prefix": "class",
        "body": "class $1 extends $0\n"
    },
    "Yuescript - Mixed Class": {
        "prefix": "class",
        "body": "class $1 using $0\n"
    },
    "Yuescript - Self Inherited": {
        "prefix": "@__inherited",
        "body": "@__inherited$0"
    },
    "Yuescript - New": {
        "prefix": "new",
        "body": "new:=>\n\t$0"
    },
    "Yuescript - Inherited": {
        "prefix": "__inherited",
        "body": "__inherited$0"
    },
    "Yuescript - Self Name": {
        "prefix": "@__name",
        "body": "@__name$0"
    },
    "Yuescript - Name": {
        "prefix": "__name",
        "body": "__name$0"
    },
    "Yuescript - Super Class": {
        "prefix": "super",
        "body": "super$0"
    }
}