{
    "Built-in - Global Environment": {
        "prefix": "_G",
        "body": "_G$0",
        "description": "represents the global Lua environment (_G._G =_G)"
    },
    "Built-in - Lua Version": {
        "prefix": "_VERSION",
        "body": "_VERSION$0",
        "description": "represents the current Lua version"
    },
    "Built-in - Import": {
        "prefix": "import",
        "body": "import $1 as :$0",
        "description": "imports a module, and extracts specific `const` items from it"
    },
    "Built-in - Require": {
        "prefix": "require",
        "body": "$2 = require($1)\n$0",
        "description": "imports a module, and extracts specific `const` items from it"
    },
    "Built-in - Load File": {
        "prefix": "loadfile",
        "body": "loadfile($0)",
        "description": "loads a Lua chunk from a file, but doesn't execute it"
    },
    "Built-in - Load": {
        "prefix": "load",
        "body": "load($1)"
    },
    "Built-in - Do File": {
        "prefix": "dofile",
        "body": "dofile($0)",
        "description": "Loads a Lua chunk from a file, and then executes it"
    },
    "Built-in - Load String": {
        "prefix": "loadstring",
        "body": "loadstring($0)",
        "description": "Similar to load, but gets the chunk from the given string"
    },
    "Built-in - Next": {
        "prefix": "next",
        "body": "next($0)"
    },
    "Built-in - Protected Call": {
        "prefix": "pcall",
        "body": "pcall($1, $0)"
    },
    "Built-in - Raw Equal": {
        "prefix": "rawequal",
        "body": "rawequal($1, $0)"
    },
    "Built-in - Raw Get": {
        "prefix": "rawget",
        "body": "rawget($1, $0)"
    },
    "Built-in - Raw Set": {
        "prefix": "rawset",
        "body": "rawset($1, $2, $0)"
    },
    "Built-in - Select": {
        "prefix": "select",
        "body": "select($0)"
    },
    "Built-in - Set Function Environment": {
        "prefix": "setfenv",
        "body": "setfenv($1, $0)"
    },
    "Built-in - Set Metatable": {
        "prefix": "setmetatable",
        "body": "setmetatable($1, $0)"
    },
    "Built-in - Assert": {
        "prefix": "assert",
        "body": "assert($1, $0)"
    },
    "Built-in - To Number": {
        "prefix": "tonumber",
        "body": "tonumber($0)"
    },
    "Built-in - To String": {
        "prefix": "tostring",
        "body": "tostring($0)"
    },
    "Built-in - Get Type": {
        "prefix": "type",
        "body": "type($0)"
    },
    "Built-in - Unpack": {
        "prefix": "unpack",
        "body": "unpack($0)"
    },
    "Built-in - Handled Protected Call": {
        "prefix": "xpcall",
        "body": "xpcall($1, $0)"
    },
    "Built-in - Collect Garbage": {
        "prefix": "collectgarbage",
        "body": "collectgarbage($0)"
    },
    "Built-in - Error": {
        "prefix": "error",
        "body": "error($0)"
    },
    "Built-in - Get Function Environment": {
        "prefix": "getfenv",
        "body": "getfenv($0)"
    },
    "Built-in - Get Metatable": {
        "prefix": "getmetatable",
        "body": "getmetatable($0)"
    },
    "Built-in - Iterate all Integer keys": {
        "prefix": "ipairs",
        "body": "ipairs($0)"
    },
    "Built-in - Iterate all keys": {
        "prefix": "pairs",
        "body": "pairs($0)"
    },
    "Built-in - Print": {
        "prefix": "print",
        "body": "print($0)"
    }
}