{
    "Control Flow - If": {
        "prefix": "if",
        "body": "if (${1:condition})\n\t$0"
    },
    "Control Flow - Elseif": {
        "prefix": "elseif",
        "body": "elseif (${1:condition})\n\t$0"
    },
    "Control Flow - Else": {
        "prefix": "else",
        "body": "else\n\t$0"
    },
    "Control Flow - While": {
        "prefix": "while",
        "body": "while (${1:condition})\n\t$0"
    },
    "Control Flow - Continue": {
        "prefix": "continue",
        "body": "continue\n"
    },
    "Control Flow - Break": {
        "prefix": "break",
        "body": "break $0"
    },
    "Control Flow - Return": {
        "prefix": "return",
        "body": "return $0"
    },
    "Control Flow - For ipairs": {
        "prefix": "foripairs",
        "body": "for $2,$3 in ipairs($1) do\n\t$0"
    },
    "Control Flow - For pairs": {
        "prefix": "forpairs",
        "body": "for $2,$3 in pairs($1) do\n\t$0"
    },
    "Control Flow - Pass/No Operation": {
        "prefix": ["pass", "noop"],
        "body": "(()->nil)!"
    }
}