{
    "Flow Control - If": {
        "prefix": "if",
        "body": "if (${1:condition})\n\t$0"
    },
    "Flow Control - Elseif": {
        "prefix": "elseif",
        "body": "elseif (${1:condition})\n\t$0"
    },
    "Flow Control - Else": {
        "prefix": "else",
        "body": "else\n\t$0"
    },
    "Flow Control - While": {
        "prefix": "while",
        "body": "while (${1:condition})\n\t$0"
    },
    "Flow Control - Continue": {
        "prefix": "continue",
        "body": "continue\n"
    },
    "Flow Control - Break": {
        "prefix": "break",
        "body": "break\n"
    },
    "Flow Control - Return": {
        "prefix": "return",
        "body": "return $0\n"
    },
    "Flow Control - Do": {
		"prefix": "do",
		"body": "do\n\t$0"
	},
	"Flow Control - For": {
		"prefix": "for",
		"body": "for $0"
	},
    "Flow Control - For ipairs": {
        "prefix": "foripairs",
        "body": "for $2,$3 in ipairs($1) do\n\t$0"
    },
    "Flow Control - For pairs": {
        "prefix": "forpairs",
        "body": "for $2,$3 in pairs($1) do\n\t$0"
    },
    "Flow Control - Pass/No Operation": {
        "prefix": ["pass", "noop"],
        "body": "(()->nil)!\n"
    }
}